    /// ä¸ºTM_Hideå‡†å¤‡åŒ¹é…çš„ST_Hideç‰‡æ®µ
    private func prepareSyncSTHideForTMHide(tmHide: SnoopyClip) -> SnoopyClip? {
        // æ ¹æ®æµç¨‹ç±»å‹é€‰æ‹©ST_Hideçš„ç¼–å·
        let stHideNumber: String
